<!-- 픽 사연 시작 -->
<div class="pick-ex">
    <div class="pick-container container swiper mySwiper">
        <div class="title">
            <h2>About's PICK</h2>
            <a href="#!" class="view-all">전체보기</a>
        </div>
        <div class="card-container swiper-wrapper">
            {{#clientMain.comms}}
                <a href="#!" class="card swiper-slide">
                    <div class="card-top">
                        <div class="pick-title">
                            {{title}}
                        </div>
                        <div class="category-area">
                            <div class="pick-category">{{category}}</div>
                            <div class="dot"></div>
                            <div>
                                <div class="badge">
                                    <img src=".{{writerImage}}" alt="">
                                </div>
                                <div>{{writerName}}</div>
                            </div>
                        </div>
                        <div class="pick-content">
                            {{content}}
                        </div>
                    </div>
                    <div class="card-bottom">
                        <div class="expert-img">
                            <img src="./images/{{expertImage}}" alt="expert img">
                        </div>
                        <div>{{expertName}}님의 전문가답변</div>
                    </div>
                </a>
            {{/clientMain.comms}}
        </div>
        <div class="swiper-button-next"></div>
        <div class="swiper-button-prev"></div>
        <!-- <div class="swiper-scrollbar"></div> -->
    </div>
</div>
<!-- 픽 사연 끝 -->
