{{> layout/header}}


<div class="banner-ex">
    <div class="banner-container  container swiper mySwiper">
        <div class="card-container swiper-wrapper">
            <a href="#!" class="card swiper-slide">
                <img src="./images/main01.png" alt="">
            </a>
            <a href="#!" class="card swiper-slide">
                <img src="./images/main02.png" alt="">
            </a>
            <a href="#!" class="card swiper-slide">
                <img src="./images/main03.png" alt="">
            </a>
            <a href="#!" class="card swiper-slide">
                <img src="./images/main04.png" alt="">
            </a>
        </div>
        <div class="pager"></div>
    </div>

</div>

<div class="container">
    <div class="categories mb-20">
        <ul>
            <li><a href="#!"><img src="./images/cate01.jpg" alt="">심리상담</a></li>
            <li><a href="#!"><img src="./images/cate02.jpg" alt="">코칭</a></li>
            <li><a href="#!"><img src="./images/cate03.jpg" alt="">심리검사</a></li>
            <li><a href="#!"><img src="./images/cate04.jpg" alt="">상담센터 찾기</a></li>
        </ul>
    </div>
</div>
<!-- 픽 사연 시작 -->
<section id="pick">
    <div class="container">
        <div class="pick-ex">
            <div class="pick-container container swiper mySwiper">
                <div class="title">
                    <h2 class="page-title">About's PICK</h2>
                    <a href="#!" class="view-all">전체보기</a>
                </div>
                <div class="card-container swiper-wrapper">
                    {{#clientMain.comms}}
                        <a href="/comm-detail/{{communityId}}" class="card swiper-slide">
                            <div class="card-top">
                                <div class="pick-title mb-20">
                                    {{title}}
                                </div>
                                <div class="category-area mb-10">
                                    <div class="pick-category">{{category}}</div>
                                    <div class="dot"></div>
                                    <div>
                                        <div class="badge">
                                            <img src=".{{writerImage}}" alt="">
                                        </div>
                                        <div class="name">{{writerName}}</div>
                                    </div>
                                </div>
                                <div class="pick-content">
                                    {{content}}
                                </div>
                            </div>
                            <div class="card-bottom">
                                <div class="expert-img">
                                    <img src=".{{expertImage}}" alt="expert img">
                                </div>
                                <div>{{expertName}}님의 전문가답변</div>
                            </div>
                        </a>
                    {{/clientMain.comms}}
                </div>
                <!-- <div class="swiper-scrollbar"></div> -->
            </div>
            <div class="swiper-button-next"></div>
            <div class="swiper-button-prev"></div>
        </div>
    </div>

</section>

<!-- 픽 사연 끝 -->
