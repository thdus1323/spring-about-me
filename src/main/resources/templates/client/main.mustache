<!-- 나에게 맞는 전문가 시작 -->
<section id="expert">
    <div class="container">
        <div class="expert-ex">
            <div class="expert-container swiper mySwiper">
                <h2 class="page-title">나에게 딱 맞는 전문가</h2>
                <p class="mb-10">공인 자격을 갖춘 검증된 전문가들이 여러분을 기다리고 있어요.</p>
                <div class="card-container swiper-wrapper">
                    {{#clientMain.experts}}
                        <a href="client/findExpert/detail/{{expertId}}" class="card swiper-slide">
                            <div class="card-top">
                                <div class="expert-img mb-10">
                                    <img src=".{{profileImage}}" alt="expert img">
                                </div>
                                <div class="expert-name mb-10">{{name}}</div>
                                <div class="expert-title mb-10">{{expertTitle}}</div>
                            </div>
                            <div class="card-bottom contact">
                                {{#hasTextTherapy}}
                                    <div class="chat"><img src="/images/chat.png"></div>
                                {{/hasTextTherapy}}
                                {{#hasVoiceTherapy}}
                                    <div class="call"><img src="/images/call.png"></div>
                                {{/hasVoiceTherapy}}
                                {{#hasVideoTherapy}}
                                    <div class="video"><img src="/images/video.png"></div>
                                {{/hasVideoTherapy}}
                            </div>
                        </a>
                    {{/clientMain.experts}}
                </div>
            </div>
            <div class="swiper-button-next .expert-ex"></div>
            <div class="swiper-button-prev .expert-ex"></div>
        </div>
    </div>
</section>


